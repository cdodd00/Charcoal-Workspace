global proc x_circleZ ()
{
	source x_alignLib.mel;
	
	string $sel[] = `ls -sl`;

	for ($obj in $sel)
	{
		string $ct[] = `circle -c 0 0 0	-nr 0 0 1 -sw 360 -r 1 -d 3 -ut 0 -tol 0.01 -s 8 -ch 1 -n ($obj + "_ct")`;
		alignTrans ($obj, $ct[0]);
		alignRot ($obj, $ct[0]);
		searchReplaceNames "_jt" "" "selected";		
	}
	
}